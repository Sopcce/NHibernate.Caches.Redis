@using System
@model List<Demo.MySQL.Models.TestInfo>
    @{
        ViewBag.Title = "Index";

    }


    <h2>Index</h2>
    <h2>
        Index
        <br /> @Html.Raw(ViewBag.Name)
    </h2>
    <table style="width: 500px;">
        <thead><tr><th>Title</th><th>Body</th><th>Created</th></tr></thead>

        <tbody>
            @foreach (var post in Model)
            {
                <tr>
                    <td>@post.Id</td>
                    <td>@post.Body</td>

                </tr>
            }
        </tbody>

    </table>